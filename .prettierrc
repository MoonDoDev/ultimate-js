{
	"singleQuote": true,
	"semi": true,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"arrowParens": "always",
	"bracketSameLine": true,
	"objectWrap": "collapse",
	"singleAttributePerLine": true,
	"endOfLine": "lf"
}
